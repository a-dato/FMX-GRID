    object mmHelpStrings: TMemo
      Left = 136
      Top = 8
      Width = 65
      Height = 25
      Lines.Strings = (
        
          'ID_1000=Use this dialog box to design your EzDataset connection.' +
          '\n\n* Datalinks identify your detail tables;\n* Dataset fields a' +
          're used to set special field properties;\n* Fieldmap is used to ' +
          'connect fields and columns together.'
        ''
        'ID_1100=Datalink page'
        
          'ID_1110=Datalink tree\n\n* Add a datalink for each detail table ' +
          'from which data must be loaded.\n* Then link each tables to it'#39's' +
          ' parent by setting the '#39'unique idenitfier'#39' and '#39'parent identifie' +
          'r'#39' fields.\nTo use EzPlan-IT with self referencing tables, add a' +
          ' single datalink and set the '#39'parent identifier'#39' field to refere' +
          'nce the '#39'unique identifier'#39' field.'
        
          'ID_1120=Dataset field:\n\nThis property connects the datalink to' +
          ' a dataset. This dataset is used to fetch data from when the TEz' +
          'Dataset opens.'
        
          'ID_1130=Unique identifier:\n\nSelect or enter the name of the fi' +
          'eld(s) that make up the primary key of your dataset.'
        
          'ID_1140=Parent identifier:\n\nSelect or enter the name of your f' +
          'ield(s) that reference the '#39'unique identifier field'#39' of the pare' +
          'nt record. To use EzPlan-IT with a self referencing table, add a' +
          ' single datalink and set the parent field to the field referenci' +
          'ng the parent record.'
        ''
        
          'ID_1200=Dataset fields page:\n\nThis page displays the fieldname' +
          's for the TEzDataset and allows you to set additional properties' +
          ' for each field.'
        
          'ID_1210=Fields tree:\n\nThis tree lists the fieldnames for the T' +
          'EzDataset object. Select a field to set additional properties fo' +
          'r a field.'
        
          'ID_1220=Select this checkbox if this field may be edited when th' +
          'e dataset cursor is located on a parent record.'
        
          'ID_1230=Select this checkbox if this field may be edited when th' +
          'e dataset cursor is located on a child record.'
        
          'ID_1240=Parent formula:\n\nEnter a formula that returns a value ' +
          'for this field -instead of reading the datasetfield from the det' +
          'ail dataset- when the dataset cursor is located on a parent reco' +
          'rd.'
        
          'ID_1250=Child formula:\n\nEnter a formula that returns a value f' +
          'or this field -instead of reading the datasetfield from the deta' +
          'il dataset- when the dataset cursor is located on a child record' +
          '.'
        ''
        
          'ID_1300=Fieldmap page:\n\nUse the grid on this page to map a fie' +
          'ld of your dataset to a column of the TEzDataset object. By defa' +
          'ult, fields are mapped by name. If you set one field, then you h' +
          'ave specifiy mappings for all other fields too.\nThe fieldmap ca' +
          'n only be set in case of a Master/Detail configuration.'
        ' '
        ' '
        ' '
        ' '
        ' '
        ' ')
      TabOrder = 1
      WordWrap = False
    end
  end
